#include "Node.h"
#include "countFreq.h"
#include <iostream>

Node::Node(){
  this->letter='!';
  this->frequency=0;
}

Node::Node(int frequency){
  this->letter='!';
  this->frequency=frequency;
}

Node::Node(char letter, int frequency){
  this->letter=letter;
  this->frequency=frequency;
}
char Node::getLetter(){
  return letter;
}
int Node::getFrequency(){
  return frequency;
}
void Node::setFrequency(int frequency){
  this->frequency=frequency;
}
